# EBM Diagnosis Service Configuration
PORT=3001

# Standard Provider API Keys (Fallback Providers)
OPENAI_API_KEY=sk-your-openai-api-key-here
GOOGLE_GENERATIVE_AI_API_KEY=your-google-ai-api-key-here

# E2E Networks Configuration (Primary Provider) - Matches our code expectations
E2E_NETWORKS_API_KEY=your-e2e-networks-jwt-token-here
E2E_NETWORKS_BASE_URL=https://infer.e2enetworks.net/project/p-xxxx/genai

# E2E Model Configurations
E2E_LLM_MODEL=qwen2_5_72b_instruct
E2E_EMBEDDING_MODEL=BAAI/bge-large-en-v1.5

# Neo4j Database Configuration - Matches our code expectations
NEO4J_URL=bolt://your-neo4j-host:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password
NEO4J_DATABASE=neo4j
NEO4J_MAX_POOL_SIZE=50
NEO4J_MAX_CONNECTION_LIFETIME=3600000
NEO4J_CONNECTION_TIMEOUT=60000
NEO4J_ENCRYPTED=false

# Development Configuration
NODE_ENV=development
LOG_LEVEL=info

# Authentication Configuration
API_KEY=emb-your-api-key-here