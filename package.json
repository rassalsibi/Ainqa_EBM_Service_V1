{
  "name": "emb-diagnosis-service",
  "version": "1.0.0",
  "description": "EBM Diagnosis Service - Hono.js Neo4j RAG service for AI-powered diagnosis recommendations",
  "type": "module",
  "scripts": {
    "postinstall": "lefthook install",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --fix ."
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.6",
    "@ai-sdk/deepinfra": "^1.0.11",
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/openai": "^2.0.19",
    "@ai-sdk/openai-compatible": "^1.0.11",
    "@hono/node-server": "^1.19.0",
    "ai": "^5.0.22",
    "dotenv": "^17.2.1",
    "hono": "^4.9.4",
    "neo4j-driver": "^5.28.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^20.11.17",
    "lefthook": "^1.12.3",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
